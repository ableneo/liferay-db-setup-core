<?xml version="1.0" encoding="UTF-8" ?><setup xmlns="http://www.ableneo.com/liferay/setup">
    <configuration>
        <!-- run as automatically selected admin -->
        <run-as-user-email></run-as-user-email>
        <company>
            <companyid>1</companyid>
        </company>
    </configuration>

    <roles>
        <role name="Some Role" unspecified-attribute-test="true"></role>
    </roles>
    <resource-permissions>
        <resource resource-id="some-portlet">
            <actionId name="SOME_ACTION">
                <role name="Some Role"/>
            </actionId>
        </resource>
    </resource-permissions>
</setup>
